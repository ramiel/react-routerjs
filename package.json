{
  "name": "react-routerjs",
  "version": "0.5.2",
  "description": "React bindings for routerjs",
  "main": "dist/react-routerjs.cjs.js",
  "module": "dist/react-routerjs.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build-watch": "rollup -c -w",
    "dev": "NODE_ENV=development concurrently --kill-others \"npm run build-watch\"",
    "lint": "eslint . --ext ts,tsx",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramiel/reacct-routerjs.git"
  },
