{
  "name": "react-routerjs",
  "version": "0.1.0",
  "description": "React bindings for routerjs",
  "main": "dist/react-routerjs.cjs.js",
  "module": "dist/react-routerjs.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test-only": "NODE_ENV=test jest",
    "test-watch": "npm run test-only -- --watch",
    "test": "npm run lint && npm run test-only -- --coverage",
    "build": "NODE_ENV=production rollup -c",
    "build-watch": "rollup -c -w",
    "dev": "NODE_ENV=development concurrently --kill-others \"npm run build-watch\"",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramiel/reacct-routerjs.git"
  },
  "keywords": [
    "router",
    "react"
  ],
  "author": "Fabrizio Ruggeri <fabrizio.ruggeri@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramiel/reacct-routerjs/issues"
  },
  "homepage": "https://github.com/ramiel/reacct-routerjs#readme",
  "devDependencies": {
    "@types/react": "^16.9.1",
    "eslint": "^6.1.0",
    "rollup": "^1.19.4",
    "rollup-plugin-typescript2": "^0.22.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "react": "^16.9.0",
    "routerjs": "^1.0.10"
  }
}
